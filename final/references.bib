	Pages = {89 - 102},
	Publisher = {scielocl},
	Title = {{HortSyst: A dynamic model to predict growth, nitrogen uptake, and transpiration of greenhouse tomatoes}},
	Url = {https://scielo.conicyt.cl/scielo.php?script=sci_arttext&pid=S0718-58392019000100089&nrm=iso},
